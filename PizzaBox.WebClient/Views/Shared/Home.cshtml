@model PizzaBox.WebClient.Models.CustomerViewModel

@{
  ViewBag.Title = "Customer - Home";
  Layout = "_CustomLayout";
}

<section>
  @Model.Name
</section>

@Html.Partial("Order", Model.Order)

<section>
  <h1>Order History</h1>
   @foreach (var item in Model.OrderHistory)
  {
    <p>@item</p>
    
  }
</section>
